{
  "extends": "@sapphire/ts-config/extra-strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "removeComments": true,
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "composite": true,
    "skipDefaultLibCheck": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "rootDir": "src"
  },
  "include": [
    "src",
  ]
}
